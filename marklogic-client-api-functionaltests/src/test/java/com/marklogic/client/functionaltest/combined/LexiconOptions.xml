			<element ns="" name="double"/>
		</range>
		<range type="xs:int">
			<element ns="" name="int"/>
		</range>
	</tuples>
	<tuples name="n-way">
		<range type="xs:double">
			<element ns="" name="double"/>
		</range>
		<range type="xs:int">
			<element ns="" name="int"/>
		</range>
		<range type="xs:string">
			<element ns="" name="string"/>
		</range>
		<values-option>ascending</values-option>
