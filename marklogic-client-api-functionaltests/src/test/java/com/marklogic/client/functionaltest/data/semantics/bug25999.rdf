  <!-- Wine Classes by Color -->
  <owl:Class rdf:ID="RedWine">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Wine"/>
      <owl:Restriction>
	<owl:onProperty rdf:resource="&food;hasColor"/>
	<owl:hasValue rdf:resource="&food;Red"/>
      </owl:Restriction>
    </owl:intersectionOf>
    <rdfs:label xml:lang="en" rdf:datatype="&xsd;string"
		>Red Wine</rdfs:label>
  </owl:Class>
