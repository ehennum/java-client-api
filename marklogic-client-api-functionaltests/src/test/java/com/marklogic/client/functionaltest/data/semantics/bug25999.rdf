	<owl:onProperty rdf:resource="#hasSugar"/>
	<owl:allValuesFrom>
	  <owl:Class>
	    <owl:oneOf rdf:parseType="Collection">
	      <owl:Thing rdf:about="#Dry"/>
	      <owl:Thing rdf:about="#OffDry"/>
	    </owl:oneOf>
	  </owl:Class>
	</owl:allValuesFrom>
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  <owl:Class rdf:ID="SweetWine">
